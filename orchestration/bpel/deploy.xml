<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03"
        xmlns:pns="http://globalbooks.com/placeorder">

    <process name="pns:PlaceOrderProcess">
        <active>true</active>
        <provide partnerLink="ClientPL">
            <service name="pns:PlaceOrderService" port="PlaceOrderPort"/>
        </provide>
        <invoke partnerLink="CatalogServicePL">
            <service name="cat:CatalogService" port="CatalogPort"/>
        </invoke>
        <invoke partnerLink="OrdersServicePL">
            <service name="ord:OrdersService" port="OrdersPort"/>
        </invoke>
    </process>
</deploy>